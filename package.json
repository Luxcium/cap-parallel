{
  "name": "@luxcium/parallel-mapper",
  "version": "0.0.0-unsafe-v1.0.4x",
  "license": "MIT",
  "private": false,
  "description": "",
  "main": "./index.js",
  "types": "./typings/index.d.ts",
  "engines": {
    "node": ">= 12"
  },
  "isLibrary": true,
  "scripts": {
    "build": "parcel build --no-cache",
    "prerelease": "./scripts/prerelease.sh",
    "release": "./scripts/release.sh",
    "check": "tsc --noEmit",
    "prestart": "rm -fr ./out; tsc -b src/tsconfig.json",
    "start": "node ./out/src/utilities/sharp-phash/__sharp-phash-working.js",
    "quick:start": "node ./out/src/utilities/sharp-phash/__sharp-phash-working.js",
    "check:version": "ts-node -vv;tsc -v",
    "lint": "echo \"Error: no linter specified\" && exit 0",
    "test": "jest",
    "mx:prebuild": "npm run fix && npm run test || exit 7",
    "mx:build": "mkdir -p \"./out/performance\" && tsc --noEmit false --generateCpuProfile \"./out/performance\" || exit 1",
    "mx:precibuild": "npm run build || exit 2",
    "mx:cibuild": "tsc --build \"tsconfig.src-build.json\" || exit 1",
    "mx:postcibuild": " ( [ \"$(npm audit)\" = 'found 0 vulnerabilities' ] && echo 'found 0 vulnerabilities' || exit 1) &&  [ \"${ZE160DDEEx}\" = \"1050D673\" ] && (coveralls < coverage/lcov.info); exit 0",
    "mx:coverage": "jest --verbose false --silent true --coverage || exit 1",
    "mx:lint": "eslint \"./src/**/*.*\" --fix-dry-run || exit 1",
    "mx:lint:json": "eslint \"**/*.{jsonc,json}\" --fix-dry-run || exit 1",
    "mx:pretty": "prettier --config \"./.prettierrc\" --loglevel log -c \"./src/**/*.*\"|| exit 1",
    "mx:lint:fix": "eslint \"./src/**/*.*\" --fix || exit 1",
    "mx:lint:fix:json": "eslint \"**/*.{jsonc,json}\" --fix || exit 1",
    "mx:pretty:fix": "prettier --config \"./.prettierrc\" --loglevel log -c --write \"./src/**/*.*\" || exit 1",
    "mx:pretty:fix:all": "prettier --config \"./.prettierrc\" --loglevel log -c --write \"**/*.*\" || exit 1",
    "mx:pretty:all": "prettier --config \"./.prettierrc\" --loglevel log -c \"**/*.*\" || exit 1",
    "mx:prefix": "npx eslint --print-config src/index.ts | npx eslint-config-prettier-check || exit 1",
    "mx:fix": "npm run lint:fix && npm run pretty:fix || exit 4",
    "mx:test": "jest --verbose true || exit 1",
    "mx:precheck": "npm run fix || exit 5",
    "mx:check": "npm run test && tsc --noEmit --listFiles ||exit 3",
    "mx:postcheck": "npm run coverage || exit 2",
    "mx:tsc:version": "tsc --version"
  },
  "keywords": [],
  "author": "Benjamin Vincent (Luxcium)",
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/plugin-transform-typescript": "^7.16.8",
    "@types/chai": "^4.3.0",
    "@types/eslint-config-prettier": "^6.11.0",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^7.2.0",
    "@types/jest": "^27.4.1",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^17.0.21",
    "@types/prettier": "^2.4.4",
    "@types/sharp": "^0.30.0",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "am": "^1.1.0",
    "chai": "^4.3.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^38.0.6",
    "eslint-plugin-jsonc": "^2.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unicorn": "^41.0.1",
    "eslint": "^8.11.0",
    "got": "^12.0.2",
    "jest": "^27.5.1",
    "nyc": "^15.1.0",
    "prettier": "^2.6.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "dotenv": "^16.0.0",
    "fp-ts": "^2.11.9",
    "fs-extra": "^10.0.1",
    "glob": "^7.2.0",
    "hamming-distance": "^1.0.0",
    "jimp": "^0.16.1",
    "redis": "^4.0.4",
    "sharp": "^0.30.3",
    "sharp-phash": "^2.0.0",
    "tedis": "^0.1.12",
    "tslib": "^2.3.1"
  }
}
